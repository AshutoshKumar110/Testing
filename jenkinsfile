pipeline{
    agent any

    stages{
        stage ('Complie stage'){
            steps{
                withMaven(maven : 'maven3){ 
                    sh 'mvn clean compile'
                }
            }
        }
        stage ('Depend'){
            input {
                message "Do you want to proceed"
            }
            steps{
                sh 'echo "check karo"'
            }
        }
    }
}
